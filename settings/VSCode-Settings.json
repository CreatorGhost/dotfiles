{
  "workbench.colorTheme": "Winter Pearl Theme",
  "glassit.alpha": 255,
  "terminal.integrated.shell.windows": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
  "gitlens.advanced.messages": {
    "suppressLineUncommittedWarning": true
  },
  "workbench.iconTheme": "material-icon-theme",
  "cSpell.userWords": [
    "Cryptocurrency"
  ],
  "liveServer.settings.donotShowInfoMsg": true,
  "code-runner.runInTerminal": true,
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [
      80
    ],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "git.enableSmartCommit": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "liveServer.settings.donotVerifyTags": true,
  "terminal.integrated.shell.linux": "/bin/bash",
  "python.defaultInterpreterPath": "/usr/bin/python3",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "python.languageServer": "Jedi",
  "kite.showWelcomeNotificationOnStartup": false,
  "C_Cpp.updateChannel": "Insiders",
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": false,
    "markdown": true,
    "cpp": true,
    "python": true
  },
  "solidity.compileUsingRemoteVersion": "0.6.0",
  "editor.formatOnSave": true,
  "python.formatting.provider": "black",
  "[json]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "files.maxMemoryForLargeFilesMB": 6096,
  "bracket-pair-colorizer-2.depreciation-notice": false,
  "terminal.integrated.sendKeybindingsToShell": true,
  "editor.fontSize": 20,
  "terminal.integrated.fontFamily": "'SourceCodePro+PowerLine+AwesomeRegular'",
  "editor.fontFamily": "'SourceCodePro+PowerLine+AwesomeRegular'",
  "explorer.confirmDelete": false,
  "git.autofetch": true,
  "cursor.cpp.disabledLanguages": [
    "yaml",
    "plaintext"
  ],
  "workbench.colorCustomizations": {
  },
  "files.autoSave": "afterDelay",
  "json.schemas": [
    
  ]
}